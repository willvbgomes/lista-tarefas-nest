generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id     String     @id @default(uuid())
  title  String
  status TaskStatus @default(Pendente)

  @@map("tasks")
}

enum TaskStatus {
  Conclu√≠da
  Pendente
}
